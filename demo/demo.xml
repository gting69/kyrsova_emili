<odoo>
        <record id="room_main_hall" model="coworking.room">
            <field name="name">Main Hall (Open Space)</field>
            <field name="capacity">25</field>
            <field name="active">True</field>
        </record>

        <record id="room_conference" model="coworking.room">
            <field name="name">Conference Room A</field>
            <field name="capacity">10</field>
            <field name="active">True</field>
        </record>

        <record id="room_private_vip" model="coworking.room">
            <field name="name">VIP Private Office</field>
            <field name="capacity">4</field>
            <field name="active">True</field>
        </record>

        <record id="desk_premium_1" model="coworking.desk">
            <field name="name">Premium Desk P-01</field>
            <field name="room_id" ref="room_main_hall"/>
            <field name="is_premium">True</field>
        </record>

        <record id="desk_standard_1" model="coworking.desk">
            <field name="name">Standard Desk S-05</field>
            <field name="room_id" ref="room_main_hall"/>
            <field name="is_premium">False</field>
        </record>

        <record id="desk_meeting_1" model="coworking.desk">
            <field name="name">Meeting Table M-01</field>
            <field name="room_id" ref="room_conference"/>
            <field name="is_premium">False</field>
        </record>

        <record id="rate_hourly" model="coworking.rate">
            <field name="name">Hourly Standard</field>
            <field name="price">50.0</field>
        </record>

        <record id="rate_daily" model="coworking.rate">
            <field name="name">Full Day Pass</field>
            <field name="price">350.0</field>
        </record>

        <record id="rate_monthly" model="coworking.rate">
            <field name="name">Monthly Membership</field>
            <field name="price">5000.0</field>
        </record>

        <record id="service_coffee" model="coworking.service">
            <field name="name">Unlimited Coffee &amp; Tea</field>
            <field name="cost">100.0</field>
        </record>

        <record id="service_printing" model="coworking.service">
            <field name="name">B/W Printing (50 pages)</field>
            <field name="cost">50.0</field>
        </record>

        <record id="service_locker" model="coworking.service">
            <field name="name">Locker Rental</field>
            <field name="cost">200.0</field>
        </record>

        <record id="booking_demo_1" model="coworking.booking">
            <field name="partner_id" ref="base.partner_admin"/>
            <field name="room_id" ref="room_main_hall"/>
            <field name="start_date" eval="(datetime.now()).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="end_date" eval="(datetime.now() + relativedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="state">confirmed</field>
        </record>

        <record id="booking_demo_2" model="coworking.booking">
            <field name="partner_id" ref="base.partner_admin"/>
            <field name="room_id" ref="room_conference"/>
            <field name="start_date" eval="(datetime.now() + relativedelta(days=2)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="end_date" eval="(datetime.now() + relativedelta(days=2, hours=3)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="state">draft</field>
        </record>

        <record id="booking_demo_3" model="coworking.booking">
            <field name="partner_id" ref="base.partner_admin"/>
            <field name="room_id" ref="room_private_vip"/>
            <field name="start_date" eval="(datetime.now() + relativedelta(days=5)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="end_date" eval="(datetime.now() + relativedelta(days=6)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="state">confirmed</field>
        </record>
</odoo>
